{
  "name": "Feature Policy Control",
  "version": "0.2",
  "description": "Control Feature-Policy response headers.",
  "manifest_version": 2,
  "permissions": ["webRequest", "webRequestBlocking", "storage", "https://*/*", "http://*/*"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": { 
    "128": "icon128.png" 
  },
  "background": {
    "scripts": ["common.js", "background.js"]
  },
  "options_ui": {
    "open_in_tab": true,
    "page": "options.html"
  }
}