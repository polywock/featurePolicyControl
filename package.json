{
  "version": "1.0.0",
  "scripts": {
    "build:dev": "rm -rf build && webpack --config webpack.dev.js && cp -r static/. build/unpacked && find build -name '.DS_Store' -type f -delete",
    "build:prod": "rm -rf build && webpack --config webpack.prod.js && cp -r static/. build/unpacked && find build -name '.DS_Store' -type f -delete && cd build/unpacked && zip -r ../packed.zip ."
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-minify": "^0.5.1",
    "css-loader": "^3.4.2",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@types/chrome": "0.0.93",
    "@types/lodash.debounce": "^4.0.6",
    "immer": "^8.0.1",
    "lodash.debounce": "^4.0.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.9.0",
    "regenerator-runtime": "^0.13.5"
  }
}
